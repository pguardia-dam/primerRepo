<Window x:Class="projecte01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projecte01"
        mc:Ignorable="d"
        Title="PrimeraInterficie" Height="600" Width="525">
    <StackPanel>
        <Label Content="Nom: "></Label>
        <TextBox Width="523" Height="30" Text="{Binding Path=nom, Mode=TwoWay}"></TextBox>
        <Label Content="Cognom: "></Label>
        <TextBox Width="523" Height="30" Text="{Binding Path=nom, Mode=TwoWay}"></TextBox>
        <Label Content="Nom Complert: "></Label>
        <TextBox Width="523" Height="30" Text="{Binding Path=nom, Mode=TwoWay}"></TextBox>
        
    </StackPanel>
</Window>
